{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.content.pageFolders": [
    {
      "title": "drafts",
      "path": "[[workspace]]/collections/_drafts"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/collections/_posts"
    },
    {
      "path": "[[workspace]]/basic-rules",
      "title": "basic-rules"
    },
    {
      "path": "[[workspace]]/collections/_spells",
      "title": "_spells"
    },
    {
      "path": "[[workspace]]/collections/_races",
      "title": "_races"
    },
    {
      "path": "[[workspace]]/collections/_one-shot",
      "title": "_one-shot"
    },
    {
      "path": "[[workspace]]/collections/_monsters",
      "title": "_monsters"
    },
    {
      "path": "[[workspace]]/collections/_classes",
      "title": "_classes"
    },
    {
      "path": "[[workspace]]/collections/_magic-items",
      "title": "_magic-items"
    },
    {
      "path": "[[workspace]]/collections/_equipments",
      "title": "_equipments"
    },
    {
      "path": "[[workspace]]/collections/_backgrounds",
      "title": "_backgrounds"
    }
  ]
}